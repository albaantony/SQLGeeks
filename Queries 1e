SELECT P.Name,
		Pe.FirstName,
		Pe.LastName, 
		Tp.PaymentAmount
FROM 
		[dbo].[Property] P 
				INNER JOIN 
						[dbo].[OwnerProperty] O 
				ON P.Id = O.Id
				INNER JOIN 
						[dbo].[Person] Pe
				ON P.Id = Pe.Id
				INNER JOIN
						[dbo].[TenantProperty] Tp
				ON O.PropertyId = Tp.PropertyId
WHERE O.OwnerId = 1426
